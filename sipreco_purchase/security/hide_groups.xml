<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Hide unused groups to make user configuration easier -->

    <!-- warehouse manager -->
    <record id="stock.group_stock_manager" model="res.groups">
        <field name="category_id" ref="base.module_category_hidden"/>
        <!-- cambiamos name para que no de error de unicidad -->
        <field name="name">Stock Manager</field>
    </record>

    <!-- purchase manager -->
    <record id="purchase.group_purchase_manager" model="res.groups">
        <field name="category_id" ref="base.module_category_hidden"/>
        <!-- cambiamos name para que no de error de unicidad -->
        <field name="name">Purchase Manager</field>
    </record>

</odoo>
