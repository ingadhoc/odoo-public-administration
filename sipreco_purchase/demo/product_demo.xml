        <field name="route_ids" eval="[(4, ref('stock_location_rute_economato'))]"/>
    </record>

    <record id="product_of_economato_2" model="product.product">
        <field name="name">Producto de Economato 2</field>
        <field name="type">product</field>
        <field name="standard_price" eval="130.0"/>
        <field name="route_ids" eval="[(4, ref('stock_location_rute_economato'))]"/>
    </record>

    <record id="product_of_secretarias" model="product.product">
        <field name="name">Producto de Secretarías</field>
        <field name="type">product</field>
        <field name="standard_price" eval="1.0"/>
        <field name="route_ids" eval="[(6, False, [ref('stock_location_rute_sec_parlamentaria'), ref( 'stock_location_rute_sec_administrativa')])]"/>
    </record>

</odoo>
