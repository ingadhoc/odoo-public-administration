<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="product_of_informatica_1" model="product.product">
        <field name="name">Producto de Informática 1</field>
        <field name="type">product</field>
        <field name="standard_price" eval="100.0"/>
        <field name="route_ids" eval="[(4, ref('stock_location_rute_informatica'))]"/>
    </record>

    <record id="product_of_economato_1" model="product.product">
        <field name="name">Producto de Economato 1</field>
        <field name="type">product</field>
