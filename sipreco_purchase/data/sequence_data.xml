<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for requests -->
        <record id="seq_procurement_request" model="ir.sequence">
            <field name="name">Procurement Request</field>
            <field name="code">stock.procurement.request</field>
            <field name="prefix">PR-%(year)s-</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">no_gap</field>
        </record>

    </data>
</odoo>
