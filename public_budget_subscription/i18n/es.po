#. module: public_budget_subscription
#: model:mail.template,body_html:public_budget_subscription.purchase_account_analytic_cron_email_template
msgid ""
"\n"
"Hello ${object.name},\n"
"\n"
"% macro account_table(values):\n"
"<table cellspacing=\"1\" border=\"1\" cellpadding=\"4\">\n"
"    <tr>\n"
"        <th>Supplier</th>\n"
"        <th>Code</th>\n"
"        <th>Contract</th>\n"
"        <th>Dates</th>\n"
"        <th>Prepaid Units</th>\n"
"        <th>Contact</th>\n"
"        <th>Expedient</th>\n"
"    </tr>\n"
"    % for partner, accounts in values:\n"
"        % for account in accounts:\n"
"        <tr>\n"
"            <td>${partner.name}</td>\n"
"            <td>${account.code or ''}</td>\n"
"            <td><a href=\"${ctx.get('base_url')}/web#action=${ctx.get('action_id')}&id=${account.id}&view_type=form\">${account.name}</a></td>\n"
"            <td>${account.date_start} to ${account.date and account.date or '???'}</td>\n"
"            <td>\n"
"            % if account.quantity_max != 0.0:\n"
"                ${account.remaining_hours}/${account.quantity_max} units\n"
"            % endif\n"
"            </td>\n"
"            <td>${account.partner_id.phone or ''}, ${account.partner_id.email or ''}</td>\n"
"            <td>${account.expedient_id.number or ''}</td>\n"
"        </tr>\n"
"        % endfor\n"
"    % endfor\n"
"</table>\n"
"% endmacro\n"
"\n"
"% if 'new' in ctx.get('data', {}):\n"
"    <h2>The following subscription just expired: </h2>\n"
"    ${account_table(ctx.get('data')['new'].iteritems())}\n"
"% endif\n"
"\n"
"% if 'old' in ctx.get('data', {}):\n"
"    <h2>The following expired subscription are still not processed: </h2>\n"
"    ${account_table(ctx.get('data')['old'].iteritems())}\n"
"% endif\n"
"\n"
"% if '30days' in ctx.get('data', {}):\n"
"    <h2>The following subscription will expire in less than one month: </h2>\n"
"    ${account_table(ctx.get('data')['30days'].iteritems())}\n"
"% endif\n"
"\n"
"% if '60days' in ctx.get('data', {}):\n"
"    <h2>The following subscription will expire in less than two month: </h2>\n"
"    ${account_table(ctx.get('data')['60days'].iteritems())}\n"
"% endif\n"
"\n"
"% if '90days' in ctx.get('data', {}):\n"
"    <h2>The following subscription will expire in less than three month: </h2>\n"
"    ${account_table(ctx.get('data')['90days'].iteritems())}\n"
"% endif\n"
"\n"
"<p>\n"
"    You can check all contracts to be renewed using the menu:\n"
"</p>\n"
"<ul>\n"
"    <li>Purchase / Contracts / Contracts</li>\n"
"</ul>\n"
"<p>\n"
"    Thanks,\n"
"</p>\n"
"\n"
"<pre>\n"
"--\n"
"Odoo Automatic Email\n"
"</pre>\n"
"\n"
"            "
msgstr ""
"\n"
"Hola ${object.name},\n"
"\n"
"% macro account_table(values):\n"
"<table cellspacing=\"1\" border=\"1\" cellpadding=\"4\">\n"
"    <tr>\n"
"        <th>Proveedor</th>\n"
"        <th>Referencia/Resolucion</th>\n"
"        <th>Contrato</th>\n"
"        <th>Fechas</th>\n"
"        <th>Unidades</th>\n"
"        <th>Contacto</th>\n"
"        <th>Expediente</th>\n"
"    </tr>\n"
"    % for partner, accounts in values:\n"
"        % for account in accounts:\n"
"        <tr>\n"
"            <td>${partner.name}</td>\n"
"            <td>${account.code or ''}</td>\n"
"            <td><a href=\"${ctx.get('base_url')}/web#action=${ctx.get('action_id')}&id=${account.id}&view_type=form\">${account.name}</a></td>\n"
"            <td>${account.date_start} to ${account.date and account.date or '???'}</td>\n"
"            <td>\n"
"            % if account.quantity_max != 0.0:\n"
"                ${account.remaining_hours}/${account.quantity_max} units\n"
"            % endif\n"
"            </td>\n"
"            <td>${account.partner_id.phone or ''}, ${account.partner_id.email or ''}</td>\n"
"            <td>${account.expedient_id.number or ''}</td>\n"
"        </tr>\n"
"        % endfor\n"
"    % endfor\n"
"</table>\n"
"% endmacro\n"
"\n"
"% if 'new' in ctx.get('data', {}):\n"
"    <h2>La siguiente suscripción acaba de vencer: </h2>\n"
"    ${account_table(ctx.get('data')['new'].iteritems())}\n"
"% endif\n"
"\n"
"% if 'old' in ctx.get('data', {}):\n"
"    <h2>La siguiente suscripción caducada aún no se procesa: </h2>\n"
"    ${account_table(ctx.get('data')['old'].iteritems())}\n"
"% endif\n"
"\n"
"% if '30days' in ctx.get('data', {}):\n"
"    <h2>La siguiente suscripción caducará en menos de un mes: </h2>\n"
"    ${account_table(ctx.get('data')['30days'].iteritems())}\n"
"% endif\n"
"\n"
"% if '60days' in ctx.get('data', {}):\n"
"    <h2>La siguiente suscripción caducará en menos de dos meses: </h2>\n"
"    ${account_table(ctx.get('data')['60days'].iteritems())}\n"
"% endif\n"
"\n"
"% if '90days' in ctx.get('data', {}):\n"
"    <h2>La siguiente suscripción caducará en menos de tres meses: </h2>\n"
"    ${account_table(ctx.get('data')['90days'].iteritems())}\n"
"% endif\n"
"\n"
"<p>\n"
"    Puede verificar todos los contratos que se renovarán utilizando el menú:\n"
"</p>\n"
"<ul>\n"
"    <li>Compras / Contratos / Contrato</li>\n"
"</ul>\n"
"<p>\n"
"    Gracias,\n"
"</p>\n"
"\n"
"<pre>\n"
"--\n"
"Odoo Automatic Email\n"
"</pre>\n"
"\n"
"            "

