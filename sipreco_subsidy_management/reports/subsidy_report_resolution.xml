<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

<!-- Printed Invoice -->
<record id="action_aeroo_subsidy_resolution_report" model="ir.actions.report.xml">
    <field name="name">Reporte Tabla Resoluciones de Subsidios</field>
    <field name="type">ir.actions.report.xml</field>
    <field name="model">public_budget.subsidy.resolution</field>
    <field name="report_name">subsidy_resolution_report</field>
    <field name="report_type">aeroo</field>
    <field name="in_format">oo-ods</field>
    <field name="parser_loc">l10n_ar_aeroo_base/parser.py</field>
    <field name="parser_state">loc</field>
    <field name="report_rml">sipreco_subsidy_management/reports/report_subsidy_resolution.ods</field>
    <field name="tml_source">file</field>
    <field name="out_format" ref="report_aeroo.report_mimetypes_xls_odt"/>
    <field name="styles_mode">specified</field>
    <field name="stylesheet_id" ref="public_budget.sipreco_stylesheet"/>
</record>

<ir_set>
    <field eval="'action'" name="key"/>
    <field eval="'client_print_multi'" name="key2"/>
    <field eval="['public_budget.subsidy.resolution']" name="models"/>
    <field name="name">asset</field>
    <field eval="'ir.actions.report.xml,'+str(action_aeroo_subsidy_resolution_report)" name="value"/>
    <field eval="True" name="isobject"/>
    <field eval="True" name="replace"/>
</ir_set>


