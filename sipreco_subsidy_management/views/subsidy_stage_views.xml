<?xml version='1.0' encoding='utf-8'?>
<odoo>    
    <record id="helpdesk_subsidy_stage_view_form_inherit" model="ir.ui.view">
        <field name="name">helpdesk_subsidy_stage_view_form_inherit</field>
        <field name="model">helpdesk.stage</field>
        <field name="inherit_id" ref="helpdesk.helpdesk_stage_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='is_close']" position="after">
                <field name="is_verified"/>                
            </xpath>            
        </field>
    </record>
</odoo>
