            <field name="sequence" eval="70"/>
        </record>

        <record id="seq_approval_arrangement" model="ir.sequence">
            <field name="name">Disposición de aprobacion</field>
            <field name="code">approval_arrangement</field>
            <field name="suffix">/%(y)s</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
