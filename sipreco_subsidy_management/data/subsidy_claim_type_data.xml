            <field name="code">1</field>
            <field name="sequence" eval="10"/>
        </record>
        <record id="claim_type_2" model="public_budget.subsidy.claim.type">
            <field name="name">NOTA A PRESIDENCIA- Omisión presentación de rendición de cuentas</field>
            <field name="code">2</field>
            <field name="sequence" eval="20"/>
        </record>


        <record id="claim_type_3" model="public_budget.subsidy.claim.type">
            <field name="name">NOTA A PRESIDENCIA- Novedad por rendición presentada</field>
            <field name="code">3</field>
            <field name="sequence" eval="30"/>
        </record>

        <record id="claim_type_4" model="public_budget.subsidy.claim.type">
            <field name="name">NOTA CON OBSERVACIONES AL RESPONSABLE</field>
